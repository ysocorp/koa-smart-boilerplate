{
  "name": "my-app",
  "version": "0.2.0",
  "author": "YSO CORP",
  "description": "A RESTful API boilerplate with Koajs 2, @Decorator, Node v8, async-await, babel, and much more...",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "npm run dev",
    "prod": "node dist/index.js",
    "_rm-build-prod": "rimraf dist",
    "_build-prod": "cross-env NODE_ENV=production babel src --out-dir dist",
    "build-prod": "npm run _rm-build-prod && npm run _build-prod",
    "test": "cross-env NODE_ENV=test mocha --opts mocha.opts",
    "lint": "eslint src test"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "koa-smart": "^3.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "expect": "^24.8.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "apidoc"
    ]
  }
}
